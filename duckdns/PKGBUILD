pkgname=duckdns
pkgver=1.0
pkgrel=1
pkgdesc="Simple DDNS service"
arch=('any')
url="https://github.com/jgthomas/scripts"
license=('GPL3')
depends=('curl')
source=("duckdns.service" "duckdns.timer" "duck.log" "duck.sh")
md5sums=('SKIP' 'SKIP' 'SKIP' 'SKIP')


package() {

        install -D -m644 "$srcdir/${pkgname}" "$pkgdir/usr/bin/${pkgname}"

        chmod +x "$pkgdir/usr/bin/${pkgname}"

}


